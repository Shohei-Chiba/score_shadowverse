<p id="notice"><%= notice %></p>

<%= search_form_for @search do |f| %>
  <div class="form-group">
    <%= f.label :memo_cont, "掲示板検索" %>
    <%= f.text_field :memo_cont, class: "form-control" %>
    <%= f.label :category_cont, "カテゴリー"%>
    <%= f.select(:category_eq, ["ルームマッチ", "フレンド募集","チームメンバー募集", "対抗戦", "雑談"], include_blank: true)%>
  </div>
  <div class="actions"><%= f.submit "検索" %></div>
<% end %>

